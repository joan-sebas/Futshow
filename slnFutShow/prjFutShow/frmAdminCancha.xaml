<Page
    x:Class="prjFutShow.frmAdminCancha"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:prjFutShow"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <ComboBox Name="cmbAdminCancha"  HorizontalAlignment="Left" Margin="441,72,0,0" VerticalAlignment="Top" Height="41" Width="250" SelectionChanged="CmbAdminCancha_SelectionChanged">
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text= "{Binding Nombre}"/>
                </DataTemplate>

            </ComboBox.ItemTemplate>
        </ComboBox>
        <DatePicker HorizontalAlignment="Left" Margin="334,0,0,0" VerticalAlignment="Center" Height="65" Width="464"/>
        <CalendarDatePicker HorizontalAlignment="Left" Margin="423,398,0,0" VerticalAlignment="Top" Height="53" Width="217"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="483,162,0,0" VerticalAlignment="Top"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="798,228,0,0" VerticalAlignment="Top"/>
    </Grid>
</Page>
